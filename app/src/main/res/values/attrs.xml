<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeView">
        <attr name="time" format="integer"/>
    </declare-styleable>
    
    <declare-styleable name="CircularReveal">
        <attr name="startRadius" format="dimension" />
        <attr name="endRadius" format="dimension" />
        <attr name="centerOn" format="reference" />
    </declare-styleable>

    <declare-styleable name="StartAnimatable">
        <attr name="android:src" />
    </declare-styleable>

    <declare-styleable name="PlayPauseState">
        <attr name="state_play" format="boolean"/>
        <attr name="state_pause" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ColorPreference">
        <attr name="colorChoices" />
        <attr name="numColumns" />
    </declare-styleable>
</resources>